      "id": 5,
      "panels": [],
      "title": "Container Resource Usage",
      "type": "row"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${metricsSource}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineWidth": 1,
            "fillOpacity": 10,
            "showPoints": "never"
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 1
      },
      "id": 6,
      "options": {
        "legend": {
          "calcs": ["max", "mean"],
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${metricsSource}"
          },
          "expr": "rate(container_cpu_usage_seconds_total{namespace=~\"$namespace\", pod=~\"$pod\", instance=~\"$node\", container!=\"POD\", container!=\"\"}[5m])",
          "legendFormat": "{{namespace}}/{{pod}}/{{container}}",
          "refId": "A"
        }
      ],
      "title": "CPU Usage by Container",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${metricsSource}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineWidth": 1,
            "fillOpacity": 10,
            "showPoints": "never"
          },
          "unit": "bytes"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 1
      },
      "id": 7,
      "options": {
        "legend": {
          "calcs": ["max", "mean"],
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${metricsSource}"
          },
          "expr": "container_memory_working_set_bytes{namespace=~\"$namespace\", pod=~\"$pod\", instance=~\"$node\", container!=\"POD\", container!=\"\"}",
          "legendFormat": "{{namespace}}/{{pod}}/{{container}}",
          "refId": "A"
        }
      ],
      "title": "Memory Usage by Container",
      "type": "timeseries"
    }
  ],
